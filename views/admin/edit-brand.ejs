    <%- include('./includes/head.ejs') %>

    </head>

    <body>
        <!-- Header -->
        <%- include('./includes/nav.ejs') %>
        <!-- Header Ends -->

        <!-- Edit Brand -->
        <section id="edit-brand">
            <div class="jumbotron bg-light">
                <div class="container-md py-2 py-sm-2 py-md-3 py-lg-4 py-xl-5">
                    <div class="row gutters">
                        <div class="col-12 col-md-5">
                            <h2 class="h2 display-6">Edit Brand</h2>
                        </div>
                        <div class="col-12 col-md-7">
                            <div id="alert" class="alert py-1 py-sm-1" role="alert" hidden="hidden">
                                <div class="muted-text">Hello</div>
                                <i id="close-alert" class="far fa-times-circle"></i>
                            </div>
                        </div>
                    </div>
                    <hr class="my-sm-2 my-md-3 my-lg-4">
                    <div class="row gx-4 gy-3">
                        <div class="col-12 col-md-6">
                            <form id="brand-form">
                                <div class="row gx-4 gy-3">
                                    <div class="col-12">
                                        <div class="form-group">
                                            <label for="current_name">Current Brand Name</label>
                                            <input type="text" id="current_name" class="form-control py-1" placeholder="Brand Name&mldr;">
                                        </div>
                                    </div>
                                    <div class="col-12">
                                        <div class="form-group">
                                            <label for="name">New Brand Name</label>
                                            <input type="text" name="name" id="name" class="form-control py-1" placeholder="Enter Brand Name&mldr;">
                                        </div>
                                    </div>
                                    <div class="col-12">
                                        <div class="form-group">
                                            <button type="submit" id="edit-brand-btn" class="btn btn-primary py-1">Edit</button>
                                            <button type="button" id="cancel-edit-btn" class="btn btn-primary py-1">Cancel</button>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- Edit Brand Ends -->

        <!-- Edit Brand Custom JS -->
        <script src="/scripts/admin/edit-brand.script.js"></script>

        <!-- Footer -->
        <%- include('./includes/footer.ejs') %>
        <!-- Footer Ends -->
    </body>
</html>