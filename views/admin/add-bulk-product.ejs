    <%- include('./includes/head.ejs') %>

    </head>

    <body class="h-100">
        <!-- Header -->
        <%- include('./includes/nav.ejs') %>
        <!-- Header Ends -->

        <!-- Add Bulk Product -->
        <section id="add-bulk-product">
            <div class="jumbotron bg-light">
                <div class="container-md py-2 py-sm-2 py-md-3 py-lg-4 py-xl-5">
                    <div class="row gutters">
                        <div class="col-12 col-md-5">
                            <h2 class="h2 display-6">Add Bulk Product</h2>
                        </div>
                        <div class="col-12 col-md-7">
                            <div id="bulk-alert" class="alert py-1 py-sm-1" role="alert" hidden="hidden">
                                <div class="text-muted"></div>
                                <i id="close-bulk-alert" class="far fa-times-circle"></i>
                            </div>
                        </div>
                    </div>
                    <hr class="my-sm-2 my-md-3 my-lg-4">
                    <div id="add-bulk-product-area">
                        <div class="row gx-4 gy-3">
                            <div class="col-12 col-md-6">
                                <form id="bulk-product-form" enctype="multipart/form-data">
                                    <div class="row gx-4 gy-3">
                                        <div class="col-12">
                                            <div class="form-group">
                                                <label for="bulk_product" class="text-muted">Bulk Product CSV File</label>
                                                <input type="file" name="bulk_product" id="bulk_product" class="form-control py-1" accept=".csv, application/vnd.ms-excel">
                                            </div>
                                        </div>
                                        <div class="col-12">
                                            <div class="form-group">
                                                <button type="submit" id="add-bulk-product-btn" class="btn btn-primary py-1">Add</button>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- Add Bulk Product Ends -->

        <!-- Add Bulk Product Custom JS -->
        <script src="/scripts/admin/add-bulk-product.script.js"></script>

        <!-- Footer -->
        <%- include('./includes/footer.ejs') %>
        <!-- Footer Ends -->
    </body>
</html>